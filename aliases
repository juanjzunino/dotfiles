# Optimization
alias cplex='/Applications/CPLEX_Studio129/cplex/bin/x86-64_osx/cplex'

# Editor
alias vim='nvim'

# Open files
alias finder="open -R"

# Exa
alias ls="exa -a --icons"
alias l="exa -la"
alias tree="exa --tree"
alias mtree="exa --tree --long"

# Sane defaults
alias cp="cp -i"
alias mv="mv -i"
alias rm="rm -i"

# Git
alias gs="git status"
alias gg="git log --graph --pretty=oneline"
alias gu="git pull"

# Jupyter Notebooks
alias jn="jupyter notebook"

# Folder's shortcuts
alias desk="cd ~/Desktop"
alias loads="cd ~/Downloads"
alias docs="cd ~/Documents"
alias project="cd ~/projects"
alias resources="cd ~/Documents/resources"
alias mim="cd ~/Documents/knowledge/MiM+Analytics"
