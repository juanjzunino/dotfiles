# Listing options
alias ls="exa"
alias l="exa -la"

# Sane defaults
alias cp="cp -i"
alias mv="mv -i"
alias rm="rm -i"

# Send files to trash
alias ts="trash"

# Git
alias gs="git status"
alias gg="git graph"
alias gu="git pull"

# Jupyter Notebooks
alias jn="jupyter notebook"

# Folders
alias mim="cd ~/Documents/business-analytics"
alias book="cd ~/Documents/Books"

# Web Browsers
alias chrome="/Applications/Google Chrome.app"
alias firefox="/Applications/Firefox.app"
