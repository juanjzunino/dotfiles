[init]
	defaultBranch = main

[pull]
	ff = only

[core]
    pager = delta

[interactive]
    diffFilter = delta --color-only

[delta]
    navigate = true

[merge]
    conflictstyle = diff3

[diff]
    colorMoved = default

[delta]
    side-by-side = true
